{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": "off",
    "func-names": "off",
    "camelcase": "off",
    "import/prefer-default-export": "off"
  }
}
